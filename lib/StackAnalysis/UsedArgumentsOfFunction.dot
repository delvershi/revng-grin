# This analysis works both for registers and stack slots

digraph UsedArgumentsOfFunction {
  Unknown;
  Maybe [peripheries=2];
  Yes;

  # Lattice
  Unknown->Maybe;
  Maybe->Yes;

  # Transfer functions
  Maybe->Yes [label="Read"];
  Maybe->Unknown [label="Write"];
  Maybe->Unknown [label="UnknownFunctionCall"];
}
